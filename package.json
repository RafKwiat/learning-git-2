{
  "name": "learning-git-2",
  "version": "1.0.0",
  "description": "https://dedork.github.io/learning-git-2/",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:html",
    "test:html": "globstar nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf css",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "onchange -d 100 sass/style.scss -- npm-run-all watch-*",
    "watch:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch:autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.css",
    "watch:browsersync": "node_modules/.bin/browser-sync start --server --files css/*.css *.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dedork/learning-git-2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dedork/learning-git-2/issues"
  },
  "homepage": "https://github.com/dedork/learning-git-2#readme",
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "globstar": "^1.0.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^5.2.0"
  },
  "dependencies": {
    "mkdirp": "^0.5.1",
    "nu-html-checker": "^0.1.0",
    "rimraf": "^2.6.3",
    "save-dev": "^2.0.0"
  }
}
